(mod (alice alice_pubkey bob bob_pubkey amount seconds person)
     (if (= alice person)
         (list ;; Originator can recover or spend until the coin is spent by other person.
          (c (q . 50) (c alice_pubkey (c alice (q . ()))))
          (c (q . 51) (c alice (c amount (q . ()))))
          )
         (list
          (c (q . 50) (c bob_pubkey (c bob (q . ()))))
          (c (q . 80) (c seconds (q . ())))
          (c (q . 51) (c bob (c amount (q . ()))))
          )
         )
     )
